{
    "name": "angular-universal-starter",
    "version": "0.0.0",
    "scripts": {
        "ng": "ng",
        "dev": "ng serve",
        "build": "ng build",
        "docker-dev": "docker-compose up --build",
        "docker-build": "docker-compose -f docker-compose.main.yml up --build",
        "prettier": "prettier --check 'src/**/*.ts'",
        "lint": "ng lint",
        "dev:ssr": "ng run affluences-display:serve-ssr",
        "serve:ssr": "node dist/affluences-display/server/main.js",
        "build:ssr-prod": "ng run affluences-display:build:production && ng run affluences-display:server:production",
        "build:ssr-staging": "ng run affluences-display:build:staging && ng run affluences-display:server:staging",
        "release": "export BABEL_DISABLE_CACHE=1 && release-it --ci",
        "release:dry": "export BABEL_DISABLE_CACHE=1 && release-it --ci --dry-run=true",
        "post:release": "echo Successfully released $npm_package_version",
        "build:changelog": "conventional-changelog -i CHANGELOG.md -s"
    },
    "private": true,
    "dependencies": {
        "@angular/animations": "11.2.1",
        "@angular/cdk": "11.2.1",
        "@angular/common": "11.2.1",
        "@angular/compiler": "11.2.1",
        "@angular/core": "11.2.1",
        "@angular/forms": "11.2.1",
        "@angular/material": "^11.2.1",
        "@angular/platform-browser": "11.2.1",
        "@angular/platform-browser-dynamic": "11.2.1",
        "@angular/platform-server": "11.2.1",
        "@angular/router": "11.2.1",
        "@nguniversal/express-engine": "11.2.0",
        "@schematics/angular": "10.2.2",
        "bootstrap": "4.6.0",
        "eslint-config-airbnb-typescript": "^12.3.1",
        "express": "4.17.1",
        "font-awesome": "4.7.0",
        "ngx-bootstrap": "6.2.0",
        "rxjs": "6.6.3",
        "tslib": "2.1.0",
        "zone.js": "0.11.4"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "0.1102.1",
        "@angular-eslint/builder": "1.2.0",
        "@angular-eslint/eslint-plugin": "1.2.0",
        "@angular-eslint/eslint-plugin-template": "1.2.0",
        "@angular-eslint/schematics": "1.2.0",
        "@angular-eslint/template-parser": "1.2.0",
        "@angular/cli": "11.2.1",
        "@angular/compiler-cli": "11.2.1",
        "@angular/language-service": "11.2.1",
        "@nguniversal/builders": "11.2.0",
        "@types/express": "4.17.11",
        "@types/jasmine": "3.6.4",
        "@types/jasminewd2": "2.0.8",
        "@types/node": "14.14.31",
        "@typescript-eslint/eslint-plugin": "^4.15.1",
        "@typescript-eslint/parser": "4.3.0",
        "codelyzer": "6.0.1",
        "conventional-changelog-cli": "^2.1.1",
        "cypress": "^6.5.0",
        "eslint": "^7.6.0",
        "eslint-config-prettier": "^8.0.0",
        "eslint-plugin-import": "^2.22.1",
        "eslint-plugin-jsdoc": "30.7.6",
        "eslint-plugin-prefer-arrow": "1.2.2",
        "eslint-plugin-prettier": "3.1.4",
        "husky": "^4.3.8",
        "prettier": "^2.2.1",
        "release-it": "^14.4.1",
        "ts-node": "9.1.1",
        "typescript": "4.0.7"
    }
}
